# managed by Ansible
location /parker_beta {
  root /var/www/firmware/;
  index index.html;
  fancyindex on;
  fancyindex_name_length 255;
}
location /parker_stable {
  root /var/www/firmware/;
  index index.html;
  fancyindex on;
  fancyindex_name_length 255;
}


location /raw {
  alias /var/www/firmware;
  index none;
  fancyindex on;
  fancyindex_name_length 255;
}


location /firmware/ {
  alias /var/www/firmware-selector/;
  index index.html;
}


location /.git {
  deny all;
  return 404;
}
