START=99

USE_PROCD=1
PROG=/usr/sbin/nodeconfig.sh

start_service() {
echo "foo"
        procd_open_instance
        procd_set_param command /usr/sbin/nodeconfig.sh
        procd_set_param respawn
        procd_set_param stderr 1
        #procd_set_param limits core="unlimited"  #TODO set ulimit 
        procd_close_instance
}
